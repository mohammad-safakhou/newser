{
  "general": {
    "debug": false,
    "log_level": "info",
    "max_processing_time": "10m",
    "default_timeout": "30s"
  },
  "server": {
    "address": ":10001",
    "jwt_secret": "your_jwt_secret"
  },
  "llm": {
    "providers": {
      "openai": {
        "type": "openai",
        "api_key": "",
        "base_url": "https://api.openai.com/v1",
        "models": {
          "gpt-5": {
            "name": "gpt-5",
            "api_name": "gpt-4o",
            "max_tokens": 16384,
            "temperature": 0.3,
            "cost_per_1k_input": 0.01,
            "cost_per_1k_output": 0.03,
            "capabilities": [
              "planning",
              "analysis",
              "synthesis",
              "research",
              "complex_reasoning"
            ]
          },
          "gpt-5-nano": {
            "name": "gpt-5-nano",
            "api_name": "gpt-4o-mini",
            "max_tokens": 8192,
            "temperature": 0.7,
            "cost_per_1k_input": 0.001,
            "cost_per_1k_output": 0.003,
            "capabilities": [
              "research",
              "analysis"
            ]
          }
        },
        "max_retries": 3,
        "timeout": "30s"
      }
    },
    "routing": {
      "chatting": "gpt-5",
      "planning": "gpt-5",
      "analysis": "gpt-5",
      "synthesis": "gpt-5",
      "research": "gpt-5",
      "fallback": "gpt-5-nano"
    }
  },
  "telemetry": {
    "enabled": true,
    "metrics_port": 9090,
    "log_file": "./logs/telemetry.log",
    "cost_tracking": true
  },
  "agents": {
    "max_concurrent_agents": 5,
    "agent_timeout": "2m",
    "max_retries": 3,
    "confidence_threshold": 0.7
  },
  "sources": {
    "newsapi": {
      "api_key": "",
      "endpoint": "https://newsapi.org/v2",
      "max_results": 50
    },
    "web_search": {
      "brave_api_key": "",
      "serper_api_key": "",
      "max_results": 20,
      "timeout": "30s"
    },
    "social_media": {
      "enabled": false
    },
    "academic": {
      "enabled": false
    }
  },
  "storage": {
    "redis": {
      "host": "localhost",
      "port": "6379",
      "password": "",
      "db": 0,
      "timeout": "5s"
    },
    "file": {
      "data_dir": "./data",
      "log_dir": "./logs"
    },
    "postgres": {
      "url": "",
      "host": "localhost",
      "port": "5432",
      "user": "newser",
      "password": "newser",
      "dbname": "newser",
      "sslmode": "disable",
      "timeout": "5s"
    }
  }
}


